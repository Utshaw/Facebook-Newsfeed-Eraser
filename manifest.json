{

    "manifest_version": 2,
    "name": "Facebook-Newsfeed-Eraser",
    "version": "1.0",
  
    "description": "Erase facebook newsfeed content",
  
    "icons": {
      "48": "icons/icon.svg",
      "96": "icons/icon.svg"
    },
  
    "content_scripts": [
      {
        "matches": ["*://facebook.com/*"],
        "js": ["fb.js"]
      }
    ]
  
  }